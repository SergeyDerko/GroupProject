using System.Threading.Tasks;

namespace Цикл_for
{
    class Program
    {
        // Даний метод виводить таблицю множення
        // розміром b * b
        static void tab(byte b)
        {
            Console.WriteLine("Таблиця множення {0} * {0}\n", b);
            // Цикл проходить по строках
            for (int i = 1; i <= b; i++);
            {
            // Цикл проходить по стовпцях
                for (int j = 1; j <= b; j++);
                Console.Write("{0}\t", j*i);
                Console.WriteLine();
            }
            Console.WriteLine();

        }
    }
}
