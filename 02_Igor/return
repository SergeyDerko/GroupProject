using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
          int result = Sum(230);
		  Console.WriteLine("Сума парних чисел від 1 до 230 рівна: " + result);
		  
		  Console.ReadLine();
		 		
	    	}
		
		    //Метод, який повертає суму всіх парних чисел
		    //від 1 до s
		    static int Sum(int s)
	    	{
		    int mySum = 0;
		    for (int = 1; i <=s; i++)
		      if (i%2 == 0)
			     mySum += 1;
			  return mySum;	 
		    }
		
	  }	
 }
