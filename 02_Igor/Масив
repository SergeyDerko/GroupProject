using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
		// об'являємо масив	
		int[] myArr = new int[5];
		
		//Ініціалізуєм кожен елемент масиву вручну
		myArr[0] = 100;
		myArr[1] = 23;
		myArr[2] = 25;
		myArr[3] = 31;
		myArr[4] = 1;
		
		foreach (int i in myArr)
		   Console.WriteLine(i);
		   
		Console.ReadLine();

        }
    }
}
 
