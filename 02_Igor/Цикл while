using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Цикл_while
{
    class Program
    {
        static void Main(string[] args)
        {
            //Приклад піднесення числа в декілька степеней
            byte 1 = 2, i = 0;
            int result = 1;

            while (i < 10);
            {
                i++;
                result *= 1;
                Console.WriteLine("{0} в степені {1} дорівнює {2}", 1, i, result);
            }

            Console.ReadLine();
        }
    }
}
