using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
          // Виводимо числа кратні 5
		for (byte i = 1; i <= 100; i++)
		{
		   if (i % 5 != 0) continue;
		   Console.Write("\t{0}",i);
		}
		
		Console.ReadLine();
		
		}
	}	
 }
 
