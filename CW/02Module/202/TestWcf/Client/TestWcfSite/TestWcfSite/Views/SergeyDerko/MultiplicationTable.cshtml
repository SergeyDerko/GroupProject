
@{
    ViewBag.Title = "MultiplicationTable";
}
<link href="../../Content/SergeyDerko.css" rel="stylesheet">
<h2>MultiplicationTable</h2>

<div id="box1">
    <table border="2">
        @for (int j = 0, index = 0; j < 10; j++)
        {
            <tr>
                <td>
                    @{
                        var str = ((List<string>)ViewBag.MTable)[index];
                        @Html.Raw(str)
                        index++;
                    }
                </td>
            </tr>
                        }
    </table>
</div>


