using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
		//Оголошуємо два масива
		int[] myArr = new int[5];
		int[,] TwoArr = new int[5, 6];
		int sum = 0;
		
		Random ran = new Random();
		
		//Ініціалізуємо масиви
		for (int i=1; i <=5; i++)
		{
		   myArr[i - 1] = ran.Next(1, 20);
		   for (int j=1; j<=6; j++)
		   myTwoArr [j - 1] = ran.Next(1, 30);
		}
		
		//Вираховуємо квадрат кожного елементу масива
		foreach (int fVar in myArr)
		   Console.WriteLine("{0} в квадраті дорівнює {1}", fVar, fVar*fVar);
		   
		Console.WriteLine();
		//Вирахуємо суму елементів багатомірного масиву
		foreach (int fTwoVar in myTwoArr)
	       sum +=fTwoVar;

		Console.WriteLine("Сума елементів багатомірного масиву: {0}", sum);
		
		Console.ReadLine();
		}
	}	
 }
